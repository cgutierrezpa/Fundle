      "n_current_funding": {
        "type": [
          "integer",
          "null"
        ],
        "format": "uint32",
        "example": 0
      },
      "n_target_funding": {
        "type": [
          "integer",
          "null"
        ],
        "format": "uint32",
        "example": 50000
      }
    }
  },
