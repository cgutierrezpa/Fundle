{
  "Service": {
    "type": "object",
    "required": [
      "fk_provider",
      "tx_name"
    ],
    "properties": {
      "tx_cif": {
        "type": "string",
        "example": "A05732B"
      },
      "fk_provider": {
        "type": "integer",
        "format": "uint32",
        "example": 1
      },
      "tx_name": {
        "type": "string",
        "example": "Test service"
      },
      "fk_headquarters": {
        "type": "integer",
        "format": "uint32",
        "example": 1
      },
      "url_main_image": {
        "type": "string"
      },
      "url_main_video": {
        "type": "string"
      }
    }
  },
  "CreateServiceResponse": {
    "type": "object",
    "properties": {
      "serviceId": {
        "type": "integer",
        "format": "uint32"
      }
    }
  }
}